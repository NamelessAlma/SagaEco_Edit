<?xml version="1.0" encoding="gbk"?>
<SagaAPI>
  <!-- listen address **DO NOT ADD / AT THE END** -->
  <Prefixes>http://localhost</Prefixes>
  
  <!-- connect port-->
  <Port>8001</Port>
  
  <!-- API Key -->
  <APIKey>1234567890</APIKey>

  <!-- address of Mysql server or Path to Save for dat implementation-->
  <dbhost>127.0.0.1</dbhost>
  
  <!-- database name for SQL -->
  <dbname>sagaeco</dbname>

  <!-- port for databaser server(SQL or db4o) -->
  <dbport>3306</dbport>

  <!-- 
            Info = 1,
            Warning = 2,
            Error = 4,
            SQL = 8,
            Debug = 16       
  -->
  <LogLevel>31</LogLevel>

  <dbuser>root</dbuser>
  <dbpass>123456</dbpass>

  <!-- Password used for verify the connection from mapserver -->
  <Password>saga</Password>

  <!--
  MYSQL = 0
  ACCESS = 1
  -->  
  <DBType>0</DBType>
  
  <dbEncoding>gbk</dbEncoding>

</SagaAPI>